<jboss xmlns="http://www.jboss.com/xml/ns/javaee"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.jboss.com/xml/ns/javaee
                           http://www.jboss.org/j2ee/schema/jboss_5_0.xsd"
       version="3.0">

	<enterprise-beans>
		<session>
			<ejb-name>ConfigPrinterBean</ejb-name>
			<jndi-name>MyConfigPrinter</jndi-name>
			<local-jndi-name>MyLocalConfigPrinter</local-jndi-name>
		</session>
		<session>
			<ejb-name>OverridePrinterBean</ejb-name>
			<jndi-name>OverriddenGlobally</jndi-name>
			<local-jndi-name>OverriddenLocally</local-jndi-name>
		</session>
	</enterprise-beans>
</jboss>